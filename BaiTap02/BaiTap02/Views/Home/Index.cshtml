@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Bài Tập 02</h1>
    @for(int i=2;i<=9;i++)
    {
        <hr/>
        <h2 style="color:darkblue">Bảng cửu chương @i</h2>
        @for (int j = 1; j <= 10; j++)
        {
            <p>@i x @j=@(i*j)</p>
        }

    }
</div>
